{
  "name": "game",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/electron/main.js",
 "scripts": {
  "build:renderer": "vite build",
  "build:electron": "tsc -p tsconfig.node.json",
  "build": "npm run build:renderer && npm run build:electron",
  "desktop": "npm run build && electron .",
  "dist:win": "npm run build && electron-builder --win"
  },
  "build": {
    "appId": "com.rice.minecraft",
    "productName": "Minecraft TS",
    "files": [
      "dist/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@react-three/drei": "^10.0.5",
    "@react-three/fiber": "^9.1.1",
    "axios": "^1.8.4",
    "node-persist": "^4.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.4.0",
    "remove": "^0.1.5",
    "seedrand": "^0.1.0",
    "seedrandom": "^3.0.5",
    "simplex-noise": "^4.0.3",
    "styled-components": "^6.1.16",
    "three": "^0.174.0",
    "three-mesh-bvh": "^0.9.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node-localstorage": "^1.3.3",
    "@types/node-persist": "^3.1.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/seedrandom": "^3.0.8",
    "@types/three": "^0.175.0",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
